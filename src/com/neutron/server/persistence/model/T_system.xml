<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.neutron.server.persistence.model.T_systemMapper">
    <select id="selectByID" parameterType="int" resultType="T_system">
        select * from `t_system` where T_SYSTEM_id = #{id}
    </select>
</mapper>